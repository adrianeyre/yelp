<div class="container page_box">
  <%= form_for @restaurant, html: {multipart: true} do |f| %>
    <center>
    <br><p><%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :description %><br>
    <%= f.text_area :description %><br>
    <%= f.label :image %><br>
    <%= f.file_field :image %><br>
    <%= f.submit %></p>
    </center>
  <% end %>
</div>
